<tr class="text-center">
  <td><%= user.id %></td>
  <td><%= user.name %></td>
  <td><%= user.email %></td>
  <td><%= user.role_i18n %></td>
  <td><%= user.created_at.strftime('%Y/%m/%d') %></td>
  <td>
    <ul>
      <li class='list-inline-item'>
        <%= button_to "編集", edit_admin_user_path(user), method: :get, class: "btn btn-danger" %>
      </li>
      <li class='list-inline-item'>
        <%= button_to "削除", admin_user_path(user), method: :delete, data: { confirm: '本当に削除してもよろしいですか？'}, class: "btn btn-secondary" %>
      </li>
    </ul>
  </td>
</tr>