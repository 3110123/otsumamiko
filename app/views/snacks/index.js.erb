<% if @pagy.page == 1 %>
  <% cache @snack do %>
    $('#records_table').html("<%= j(render @snack, snack: @snack)%>");
  <% end %>
<% else %>
  <% cache @snack do %>
    $('#records_table').append("<%= j(render @snack, snack: @snack)%>");
  <% end %>
<% end %>
$('#div_next_link').html("<%= j(render 'shared/next_link') %>");
$("#pageScroll").attr('data-page', "<%=@next_page%>");
